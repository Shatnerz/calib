function[] = functionGenerator(analogFrequency,analogAmplitude,type)

a = -0.001321569994774;
b=0.019198139388147;

digitalAmplitude =(analogAmplitude-a)./b;
digitalFrequency = analogFrequency;

if ~(strcmpi(type,'sine') |strcmpi(type,'triangle')|strcmpi(type,'square')|strcmpi(type,'sawtooth'))
    error('Input must be of type: sine, triangle, square or sawtooth')
end
    
if (strcmpi(type,'sine'))
    analogWriteVector(10,128+digitalAmplitude*sin(2*pi[0:frequency-1]./(frequency)));
end 

if (strcmpi(type,'triangle'))
    x=[];
    for i=1:(digitalFrequency/4+1)
       x(i) = 128+(i-1)*4*digitalAmplitude./digitalFrequency; 
    end  
    for i=digitalfrequency./4+2
        
    end
          
        analogWriteVector(10,);
    end
end

if (strcmpi(type,'triangle'))
    analogWriteVector(10,);
end

if (strcmpi(type,'triangle'))
    analogWriteVector(10,);
end



end